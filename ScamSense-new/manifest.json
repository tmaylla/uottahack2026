{
  "manifest_version": 3,
  "name": "Phish.net",
  "description": "Check if a URL or message is a scam",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "notifications"
  ],
  "host_permissions": [
    "https://ottaproxpriv.vercel.app/*"
  ]
}

